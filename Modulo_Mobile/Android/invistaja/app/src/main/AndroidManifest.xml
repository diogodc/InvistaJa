<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.invistaja.invistaja">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_invista_ja_simples"
        android:label="@string/str_nome_app"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".view.activitys.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.activitys.PrincipalActivity" />
    </application>

</manifest>